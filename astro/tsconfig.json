{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "strictNullChecks": true,
    "baseUrl": "src",
    "paths": {
      "@types/*": [
        "types/"
      ],
      "@models/*": [
        "models/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@styles/*": [
        "styles/*"
      ]
    }

  }
}